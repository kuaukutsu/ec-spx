{
  "name": "kuaukutsu/ec-spx",
  "version": "0.0.1",
  "require": {
    "php": ">=8.4",
    "ext-spx": "*",
    "php-di/php-di": "^7.0",
    "symfony/console": "^7.3"
  },
  "require-dev": {
    "8ctopus/php-spx-stubs": "^1.0.1",
    "phpbench/phpbench": "^1.4",
    "roave/security-advisories": "dev-latest"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kuaukutsu\\ec\\spx\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kuaukutsu\\ec\\spx\\tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "8.4.5"
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
